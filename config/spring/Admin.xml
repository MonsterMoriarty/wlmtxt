<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	
	
	<bean id="adminDao" class="com.wlmtxt.Admin.dao.impl.AdminDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>
	<bean id="adminService" class="com.wlmtxt.Admin.service.impl.AdminServiceImpl">
		<property name="adminDao" ref="adminDao"></property>
	</bean>
	<bean id="adminAction" class="com.wlmtxt.Admin.action.AdminAction">
		<property name="adminService" ref="adminService"></property>
	</bean>
	
	<bean id="worksExamineDao" class="com.wlmtxt.Admin.dao.impl.WorksExamineDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>
	<bean id="worksExamineService" class="com.wlmtxt.Admin.service.impl.WorksExamineServiceImpl">
		<property name="worksExamineDao" ref="worksExamineDao"></property>
		<property name="worksService" ref="worksService"></property>
	</bean>
	<bean id="worksExamineAction" class="com.wlmtxt.Admin.action.WorksExamineAction">
		<property name="worksExamineService" ref="worksExamineService"></property>
	</bean>
	
	<bean id="keywordDao" class="com.wlmtxt.Admin.dao.impl.KeywordDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>
	<bean id="keywordService" class="com.wlmtxt.Admin.service.impl.KeywordServiceImpl">
		<property name="keywordDao" ref="keywordDao"></property>
	</bean>
	<bean id="keywordAction" class="com.wlmtxt.Admin.action.KeywordAction">
		<property name="keywordService" ref="keywordService"></property>
	</bean>
	
	<bean id="adminUserDao" class="com.wlmtxt.Admin.dao.impl.AdminUserDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>
	<bean id="adminuserService" class="com.wlmtxt.Admin.service.impl.AdminUserServiceImpl">
		<property name="adminUserDao" ref="adminUserDao"></property>
	</bean>
	<bean id="AdminUserAction" class="com.wlmtxt.Admin.action.AdminUserAction">
		<property name="adminuserService" ref="adminuserService"></property>
	</bean>
	
</beans>

